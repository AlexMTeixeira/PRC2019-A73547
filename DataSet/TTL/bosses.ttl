:n12056 rdf:type owl:NamedIndividual , 
					:Boss ;
		:level 63 ;
		:health 232240 ;
		:isNPCOf :z2717 ;
		:name "Baron Geddon" .

###  http://prc.di.uminho.pt/2019/worldOfwarcraft#n12098
:n12098 rdf:type owl:NamedIndividual , 
					:Boss ;
		:level 63 ;
		:health 174180 ;
		:isNPCOf :z2717 ;
		:name "Sulfuron Harbinger" .

###  http://prc.di.uminho.pt/2019/worldOfwarcraft#n11988
:n11988 rdf:type owl:NamedIndividual , 
					:Boss ;
		:level 63 ;
		:health 327248 ;
		:isNPCOf :z2717 ;
		:name "Golemagg the Incinerator" .

###  http://prc.di.uminho.pt/2019/worldOfwarcraft#n12018
:n12018 rdf:type owl:NamedIndividual , 
					:Boss ;
		:level 63 ;
		:health 263909 ;
		:isNPCOf :z2717 ;
		:name "Majordomo Executus" .

###  http://prc.di.uminho.pt/2019/worldOfwarcraft#n11502
:n11502 rdf:type owl:NamedIndividual , 
					:Boss ;
		:level 63 ;
		:health 435451 ;
